+++set(CMAKE_CXX_IMPLICIT_LINK_DIRECTORIES "/usr/lib/gcc/x86_64-pc-linux-gnu/6.3.1;/usr/lib;/lib")
+++set(CMAKE_CXX_IMPLICIT_LINK_FRAMEWORK_DIRECTORIES "")
