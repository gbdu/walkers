+++     NOT IS_SYMLINK "$ENV{DESTDIR}${CMAKE_INSTALL_PREFIX}/bin/walkers")
+++    if(CMAKE_INSTALL_DO_STRIP)
+++      execute_process(COMMAND "/usr/sbin/strip" "$ENV{DESTDIR}${CMAKE_INSTALL_PREFIX}/bin/walkers")
+++    endif()
+++  endif()
+++endif()
